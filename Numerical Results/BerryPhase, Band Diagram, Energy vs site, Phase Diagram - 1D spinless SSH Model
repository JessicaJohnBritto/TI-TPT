      "cell_type": "markdown",
      "source": [
        "# Plotting dy vs dx"
      ],
      "metadata": {
        "id": "F_Pnf541gTT0"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Using the 2X2 Matrix\n",
        "\n",
        "def dx(k, v=1, w=2):\n",
        "    return v - w * np.cos(k)\n",
        "\n",
        "def dy(k, v=1, w=2):\n",
        "    return -w * np.sin(k)\n",
        "\n",
        "def d(k, v=1, w=2):\n",
        "    return np.sqrt(dx(k, v, w)**2 + dy(k, v, w)**2)\n",
        "\n",
        "def H(k, v=1, w=2):\n",
        "    return dx(k, v, w) * np.array([[0, 1], [1, 0]]) + dy(k, v, w) * np.array([[0, -1j], [1j, 0]])\n",
        "\n",
        "def secondcomponentInwavefucntion(k, v=1, w=2):\n",
        "    return  (dx(k, v, w) + 1j * dy(k, v, w))/(np.sqrt(2) * d(k, v, w))"
      ],
      "metadata": {
        "id": "2v0iGyNlGjBq"
      },
      "execution_count": 29,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def dxdyplot(params):\n",
        "\n",
        "  figure(figsize=(2, 2), dpi=50)\n",
        "  plt.figure()\n",
        "  ks = np.linspace(-np.pi, np.pi, 100)\n",
        "  plt.plot(\n",
        "      dx(ks, params['v'], params['w']),\n",
        "      dy(ks, params['v'], params['w']),\n",
        "      label=f\"v={params['v']}, w={params['w']}\",\n",
        "      linewidth=5,\n",
        "  )\n",
        "\n",
        "  plt.title(\"\")\n",
        "  plt.xlabel(r\"$d_x$\")\n",
        "  plt.ylabel(r\"$d_y$\")\n",
        "  plt.axhline(0, color=\"black\")\n",
        "  plt.axvline(0, color=\"black\")\n",
        "  plt.legend(loc='lower right')\n",
        "  arrow(-2.0,0,3.6,0,head_width=0.1, head_length=0.3, fc='k', ec='k')\n",
        "  arrow(0,-1.5,0,2*1.4,head_width=0.1, head_length=0.3, fc='k', ec='k')\n",
        "  plt.gca().set_aspect('equal', adjustable='box')\n",
        "  plt.show()\n",
        "\n",
        "dxdyplot(params)"
      ],

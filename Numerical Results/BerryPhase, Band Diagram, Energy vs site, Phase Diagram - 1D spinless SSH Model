      "source": [
        "# Plotting Eigenenergies as a function of v, keeping w constant"
      ],
      "metadata": {
        "id": "qTh26NHDq9o4"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def energyfunction(params1, **kwargs):\n",
        "  Nsites = params1['Nsites']\n",
        "  x = np.linspace(0,4,101)\n",
        "  energy = np.zeros([len(x),Nsites])\n",
        "\n",
        "  for i in range(len(x)):\n",
        "      energy[i] = np.linalg.eigh(Hamiltonian(params = {'w' : params1['w'], 'v' : x[i], 'Nsites' : Nsites}, pbc = \"False\"))[0]\n",
        "\n",
        "  plt.title(\"Energy Spectrum as a function of $v$ (N = {})\" .format(Nsites))\n",
        "  for i in range(Nsites):\n",
        "      plt.plot(x,energy[:,i])\n",
        "  plt.ylabel('Energy')\n",
        "  plt.xlabel('$v$')\n",
        "  plt.show()\n",
        "\n",
        "params = {\n",
        "'v' : 1,\n",
        "'w' : 2,\n",
        "'Nsites':40,\n",
        "}\n",
        "\n",
        "energyfunction(params)"
      ],
      "metadata": {

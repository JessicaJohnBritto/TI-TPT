        "def secondcomponentInwavefucntion(k, v, w):\n",
        "    return v - w * np.cos(k) -1j *w * np.sin(k)\n",
        "\n",
        "\n",
        "def plot_berry_phase(va, wa, **kwargs):\n",
        "\n",
        "  ks = np.linspace(-np.pi, np.pi, 100)\n",
        "  ang = np.zeros(len(ks))\n",
        "\n",
        "  colors = ['#55a3cd', '#4954b0', '#e96f36', '#9c2f45', '#ad1759', '#697b30', '#e13342','#c87b7c']\n",
        "  widths = [3, 3, 3, 3, 3, 3, 3, 3]\n",
        "\n",
        "  plt.figure()\n",
        "\n",
        "  for ii in range(len(va)):\n",
        "      for j in range(len(ks)):\n",
        "        ang[j] = (np.angle(secondcomponentInwavefucntion(ks[j], va[ii], wa[ii])))\n",
        "      plt.plot(ks, ang,\n",
        "              label=f\"v = {va[ii]}, w = {wa[ii]}\", linewidth=widths[ii],\n",
        "               color = colors[ii],\n",
        "              #  linestyle=styles[ii]\n",
        "               )\n",
        "\n",
        "  plt.title(\"Berry Phase vs k\")\n",
        "  plt.xlabel(\"k\")\n",
        "  plt.ylabel(\"Berry Phase\")\n",
        "  plt.legend()\n",
        "  plt.show()\n",
        "\n",
        "va1 = [ 0, 0.2, 0.4, 0.6, 0.1, 0.3, 0.5, 0.5]\n",
        "wa1 = [ 0, 0.1, 0.3, 0.5, 0.2, 0.4, 0.6, 0.5]\n",
        "\n",
        "plot_berry_phase(va1, wa1)"

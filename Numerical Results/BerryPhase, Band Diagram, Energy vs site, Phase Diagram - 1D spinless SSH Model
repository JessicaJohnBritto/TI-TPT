        "# Plotting Energy for each Site"
      ],
      "metadata": {
        "id": "o9gK3tKZQfrz"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "params = {\n",
        "'v' : 1,\n",
        "'w' : 2,\n",
        "'Nsites':40,\n",
        "}\n"
      ],
      "metadata": {
        "id": "hhmgpCGN5xmu"
      },
      "execution_count": 27,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def Hamiltonian(params, **kwargs):\n",
        "  Nsites = params['Nsites']\n",
        "  H = np.zeros([Nsites, Nsites], dtype=np.complex_)\n",
        "  H[0,1] = params['v']\n",
        "  H[Nsites-1, Nsites-2] = params['v']\n",
        "  for i in range(1, Nsites-1, 2):\n",
        "    H[i, i-1] = params['v']\n",
        "    H[i, i+1] = params['w']\n",
        "    H[i+1, i] = params['w']\n",
        "    H[i+1, i+2] = params['v']\n",
        "\n",
        "  if kwargs['pbc'] == True:\n",
        "    H[0, Nsites-1] = params['w']\n",
        "    H[Nsites-1, 0] = params['w']\n",
        "  return H\n",
        "\n",
        "H = Hamiltonian(params, pbc = False)\n",
        "plt.imshow(H.real)\n",
        "plt.colorbar()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",

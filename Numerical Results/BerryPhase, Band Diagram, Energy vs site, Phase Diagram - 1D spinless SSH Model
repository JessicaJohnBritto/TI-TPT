        "def plot_near_zero_modes(evals,evecs,params):\n",
        "\n",
        "    zero_modes_ind,numberof_zero_modes = find_near_zero_modes(evals)\n",
        "    fig = plt.figure(figsize=(8, 8))\n",
        "    plt.plot(np.abs(evecs[:, zero_modes_ind])**2)\n",
        "    plt.xlabel('Site Number',fontsize=20)\n",
        "    plt.ylabel('$|\\psi|^2$',fontsize=20)\n",
        "    plt.title('Probability distribution of Zero modes',fontsize=20, fontweight='bold')\n",
        "    plt.show()\n",
        "\n",
        "evals,evecs = np.linalg.eigh((Hamiltonian(params, pbc = False)))\n",
        "plot_near_zero_modes(evals,evecs,params)"

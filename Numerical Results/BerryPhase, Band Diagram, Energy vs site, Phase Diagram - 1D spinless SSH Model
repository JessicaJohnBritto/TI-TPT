      "cell_type": "code",
      "source": [
        "def energy_site(Hmat):\n",
        "    evals,evecs = np.linalg.eigh(Hmat)\n",
        "    evals = evals.real\n",
        "    plt.scatter(np.arange(len(evals)),evals)\n",
        "    plt.title('Energy Spectrum of Chain with {} Sites'.format(len(evals)))\n",
        "    plt.show()\n",
        "    return evals, evecs\n",
        "\n",
        "params = {\n",
        "'v' : 1,\n",
        "'w' : 2,\n",
        "'Nsites':40,\n",
        "}\n",
        "evals, evecs = energy_site(Hamiltonian(params, pbc = False))"

      "source": [
        "# Function for Dispersion Relation"
      ],
      "metadata": {
        "id": "fPnoPfk2QXfG"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# Dispersion Relation Plot for 2 bands\n",
        "def dispersionRelationSSH(v, w, Nk, **kwargs):\n",
        "  eigenvalues, eigenvectors, eigenval1, eigenval2, eigenvector1, eigenvector2= [], [], [], [], [], []\n",
        "  kk = np.linspace(-np.pi, np.pi, Nk)\n",
        "  for k in kk:\n",
        "    H = np.array([[0, v + w*np.exp(-(1j)*k)],\n",
        "             [v + w*np.exp(1j * k), 0]])\n",
        "    eigenvalue, eigenvector = LA.eigh(H)\n",
        "    eigenvalues.append(tuple(eigenvalue))\n",
        "    eigenval1.append(eigenvalue[0])\n",
        "    eigenval2.append(eigenvalue[1])\n",
        "    eigenvectors.append(eigenvector)\n",
        "    eigenvector1.append(eigenvector[0, :])\n",
        "    eigenvector2.append(eigenvector[1, :])\n",
        "  kk = kk/np.pi\n",
        "  figure(figsize=(4, 3), dpi=80)\n",
        "  plt.figure()\n",
        "  plt.plot(kk, eigenval1)\n",
        "  plt.plot(kk, eigenval2)\n",
        "  plt.ylabel(\"Energy\")\n",
        "  plt.xlabel(r'$\\frac{k}{\\pi}$')\n",
        "  plt.show()\n",
        "  pass\n",
        "\n",
        "H = dispersionRelationSSH(0, 1, 40)"
      ],
      "metadata": {
